{
  "name": "@wouterds/craftzing-slack-bot",
  "version": "1.2.0",
  "private": true,
  "scripts": {
    "postinstall": "is-ci || lefthook install",
    "build": "rm -rf ./dist && tsc",
    "patch": "npm version patch -m ':bookmark: v%s'",
    "minor": "npm version minor -m ':bookmark: v%s'",
    "major": "npm version major -m ':bookmark: v%s'",
    "dev": "nodemon",
    "lint": "eslint --ext '.ts,.js' src && tsc --noEmit",
    "lint:fix": "eslint --ext '.ts,.js' --fix src",
    "test": "jest --coverage"
  },
  "dependencies": {
    "@slack/bolt": "^3.12.2",
    "@slack/web-api": "^6.8.0",
    "axios": "^1.1.3",
    "date-fns": "^2.29.3",
    "dotenv": "^16.0.3",
    "form-data": "^4.0.0",
    "ms": "^2.1.3",
    "radash": "^9.3.0",
    "sharp": "^0.31.2",
    "string-similarity": "^4.0.4",
    "tesseract.js": "^3.0.3",
    "tsconfig-paths": "^4.1.0"
  },
  "devDependencies": {
    "@types/jest": "^29.2.3",
    "@types/ms": "^0.7.31",
    "@types/node": "^18.11.9",
    "@types/sharp": "^0.31.0",
    "@types/string-similarity": "^4.0.0",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.43.0",
    "eslint": "^8.27.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "is-ci": "^3.0.1",
    "jest": "^29.3.1",
    "lefthook": "^1.2.0",
    "nodemon": "^2.0.20",
    "prettier": "^2.7.1",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  }
}
